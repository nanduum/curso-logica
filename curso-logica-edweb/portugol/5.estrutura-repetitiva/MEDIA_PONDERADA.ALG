Algoritmo "media_ponderada"

Var

   n, i : inteiro
   x, y , z, media : real

Inicio

    escreva("Quantos casos você vai digitar: ")
    leia(n)
    
    para i de 1 ate n faca
       escreval("Digite 3 numeros: ")
       leia(x)
       leia(y)
       leia(z)
       media <- (x * 2 + y * 3 + z * 5)
       escreval("Media:",media)

       
    fimpara



Fimalgoritmo