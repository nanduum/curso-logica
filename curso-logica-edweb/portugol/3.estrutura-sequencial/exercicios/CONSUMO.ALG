Algoritmo "consumo"

Var

   distancia, combustivel, consumo : real


Inicio

      escreva("Digite a distancia percorrida (em Km): ")
      leia(distancia)
      escreva("Digite o gasto de combustivel: ")
      leia(combustivel)
      
      consumo <- distancia / combustivel
      
      escreva("Consumo Médio =",consumo:8:3)
      



Fimalgoritmo